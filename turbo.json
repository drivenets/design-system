{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"lint": {
			"dependsOn": [],
			"inputs": ["$TURBO_DEFAULT$", "$TURBO_ROOT$/eslint.config.base.ts", "$TURBO_ROOT$/tsconfig.base.json"]
		},
		"//#lint:root": {
			"inputs": ["$TURBO_DEFAULT$", "!$TURBO_ROOT$/packages/**"]
		},
		"test": {
			"dependsOn": []
		},
		"build": {
			"outputs": ["dist/**"],
			"dependsOn": ["^build"]
		}
	}
}
